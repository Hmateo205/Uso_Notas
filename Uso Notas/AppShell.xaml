<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Uso_Notas.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Uso_Notas"
    Shell.FlyoutBehavior="Disabled"
    Title="Uso_Notas"
    xmlns:views="clr-namespace:Uso_Notas.Paginas">

    <TabBar>
        <ShellContent
            Title="Notas"
            ContentTemplate="{DataTemplate Paginas:Pagina_Nota}"
            Icon="{OnPlatform 'Nota.png'}" />

        <ShellContent
            Title="Info"
            ContentTemplate="{DataTemplate Paginas:Info_Nota}"
            Icon="{OnPlatform 'Info.png'}" />
    </TabBar>

</Shell>
